@model List<TourismPlatform.Models.Feedback>

<h2>Feedback List</h2>

<table class="table table-bordered">
    <thead>
        <tr>
            <th>Message</th>
            <th>Rating</th>
            <th>Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var f in Model)
        {
            <tr>
                <td>@f.Message</td>
                <td>@f.Rating</td>
                <td>@f.CreatedAt.ToString("dd/MM/yyyy HH:mm")</td>
            </tr>
        }
    </tbody>
</table>
